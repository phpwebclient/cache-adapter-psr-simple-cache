{
  "name": "webclient/cache-adapter-psr-simple-cache",
  "type": "library",
  "description": "Interface for caching",
  "keywords": ["http-client-extension", "cache", "psr-simple-cache", "simple-cache", "psr-16"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Dudarev",
      "email": "ivan@ddrv.ru",
      "homepage": "https://ddrv.ru"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "webclient/cache-contract": "^1.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 || ^7.5 || ^8.5 || ^9.5",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "provide": {
    "webclient/cache-contract-implementation": "^1.0"
  },
  "suggest": {
    "psr/simple-cache-implementation": "Choice your favorite psr-16 implementation"
  },
  "autoload": {
    "psr-4": {
      "Webclient\\Cache\\Adapter\\PsrSimpleCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stuff\\Webclient\\Cache\\Adapter\\PsrSimpleCache\\": "stuff/",
      "Tests\\Webclient\\Cache\\Adapter\\PsrSimpleCache\\": "tests/"
    }
  }
}
